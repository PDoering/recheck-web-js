{
  "compilerOptions": {
    "module": "es6",
    "target": "es5",
    "lib": ["es6", "dom", "es2017",],
    "declaration": true,
    "outDir": "./dist/",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "types": ["cypress"]
  },
  "allowJs": true,
  "exclude": [
    "coverage",
    "node_modules",
    "!node_modules/@types",
    "dist"
  ]
}
